		"upsample" : 1,
		"connections" : {
			"in" : { 
				"audio" : [ "feed in", "fm" ],
				"midi" : [ "notes in" ]
			},
			"out" : {
				"audio" : [ "osc" ],
				"midi" : [ "through"]
			}
		},
		"groups" : [
			{ 
