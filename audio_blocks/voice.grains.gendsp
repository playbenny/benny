                    "code": "//this is grain player. it plays 2 grains at once, xfading\r\n//a lot of values are only set at grain start    \r\n\r\nBuffer mtof_lookup(\"mtof_lookup\");\r\nBuffer prm(\"voice_parameter_buffer\");\r\nBuffer detuning(\"detuning\");\r\nBuffer changed_flags(\"changed_flags\");\r\nBuffer buff(\"buff\");\r\n\r\nHistory cursor_s(0);\r\nHistory cursor_p(0);\r\n\r\nHistory a_state,a_rise,a_fall,a_e,a_pos,a_rate;\r\nHistory b_state,b_rise,b_fall,b_e,b_pos,b_rate;\r\nHistory scan,scanrate;\r\n\r\nParam voice_offset(0);\r\nParam voice_is(0, min=0,default=0,max=4096);\r\nParam p(0,min=0,default=64,max=128);\r\nParam v(0);\r\nParam panic(0);\r\n\r\nif(delta(v!=0)>0){\r\n    if(a_state==0 && b_state==0){\r\n    //note on, triggers playback if not already playing\r\n        scan = 0;\r\n        scanrate = 0.0001; //TODO\r\n        //start a\r\n        a_state = 1;\r\n        a_rise = 0.0001; //TODO\r\n        a_fall = 0.0001; //TODO\r\n        a_e = 0;\r\n    }    \r\n}\r\n\r\nif(a_state){\r\n    if(a_state == 1){\r\n        a_e += a_rise;\r\n        if(a_e>=1){\r\n            a_state = 2;\r\n            if(v!=0){\r\n                //start b\r\n                b_state = 1;\r\n                b_rise = 0.0001; //TODO\r\n                b_fall = 0.0001; //TODO\r\n                b_e = 0;\r\n            }\r\n        }\r\n    }else if(a_state == 2){\r\n        a_e -= a_rise;\r\n        if(a_e<=0){\r\n            a_e = 0;\r\n            if((b_state!=1) && (v!=0)){\r\n                //start a again\r\n                a_state = 1;\r\n                a_rise = 0.0001; //TODO\r\n                a_fall = 0.0001; //TODO\r\n                a_e = 0;\r\n            }else{\r\n                a_state = 0;\r\n            }\r\n        }\r\n    }    \r\n}\r\n\r\nif(b_state){\r\n    if(b_state == 1){\r\n        b_e += b_rise;\r\n        if(b_e>=1){\r\n            b_state = 2;\r\n            if(v!=0){\r\n                //start a\r\n                a_state = 1;\r\n      