					"code" : "Buffer mtof_lookup(\"mtof_lookup\");\r\nBuffer prm(\"voice_parameter_buffer\");\r\n//Buffer detuning(\"detuning\");\r\n\r\nHistory l1,l2,l3,l4,l5,l6,l7,l8,p3,p5,p7;\r\nHistory f1,f2,f3,f4,f5,f6,f7,f8,op,o_2,mgain;\r\n\r\nParam voice_offset(0);\r\nParam voice_is(0);\r\nParam p(0,min=0,default=64,max=128);\r\nParam v(0,min=0,default=0,max=1000);\r\nParam panic(0);\r\n\r\n\r\nif(delta(phasor(60))<0){\r\n\tmgain = 4*peek(prm,11+voice_offset,0, channels=1)-2;\r\n\tmgain *= 0.33;\r\n\tl1 = 0.001+mgain*peek(prm,voice_offset, 0, channels=1);\r\n\tl2 = 0.001+mgain*peek(prm,1+voice_offset, 0, channels=1);\r\n\tl3 = 0.001+mgain*peek(prm,2+voice_offset, 0, channels=1);\r\n\tl4 = 0.001+mgain*peek(prm,3+voice_offset, 0, channels=1);\r\n\tl5 = 0.001+mgain*peek(prm,4+voice_offset, 0, channels=1);\r\n\tl6 = 0.001+mgain*peek(prm,5+voice_offset, 0, channels=1);\r\n\tl7 = 0.001+mgain*peek(prm,6+voice_offset, 0, channels=1);\r\n\tl8 = 0.001+mgain*peek(prm,7+voice_offset, 0, channels=1);\r\n\tp3 = 2.5+peek(prm,8+voice_offset, 0, channels=1);\r\n\tp5 = 4.5+peek(prm,9+voice_offset, 0, channels=1);\r\n\tp7 = 6.5+peek(prm,10+voice_offset, 0, channels=1);\r\n\to_2 = 1+floor(9*peek(prm,12+voice_offset,0, channels=1));\r\n\r\n\top=-2;\r\n}\r\n\r\npp = p+16*in1;\r\nif(pp!=op){\r\n\top = pp;\r\n\tf1 = sample(mtof_lookup,((op+0.5)/128), (voice_is % 8), channels=1);\r\n\tf2 = 2*f1;\r\n\tf4 = 4*f1;\r\n\tf8 = 8*f1;\r\n\tf3 = p3*f1;\r\n\tf5 = p5*f1;\r\n\tf6 = 2*p3*f1;\r\n\tf7 = p7*f1;\r\n}\r\nc1 = cycle(f1);\r\nc2 = cycle(f2);\r\nc3 = cycle(f3);\r\nc4 = cycle(f4);\r\nc5 = cycle(f5);\r\nc6 = cycle(f6);\r\nc7 = cycle(f7);\r\nc8 = cycle(f8);\r\n\r\n//osc = slide(mgain,1000,1000)*(0.8+in2)*(l1*c1+l2*c2+l3*c3+l4*c4+l5*c5+l6*c6+l7*c7+l8*c8);\r\no1 = c1*slide(l1*l1*l1,100,100);\r\no2 = c2*slide(l2*l2*l2,100,100);\r\no3 = c3*slide(l3*l3*l3,100,100);\r\no4 = c4*slide(l4*l4*l4,100,100);\r\no5 = c5*slide(l5*l5*l5,100,100);\r\no6 = c6*slide(l6*l6*l6,100,100);\r\no7 = c7*slide(l7*l7*l7,100,100);\r\no8 = c8*slide(l8*l8*l8,100,100);\r\n\r\no1 = sin(o1)/(l1);\r\no2 = sin(o2)/(l2);\r\no3 = sin(o3)/(l3);\r\no4 = sin(o4)/(l4);\r\no5 = sin(o5)/(l5);\r\no6 = sin(o6)/(l6);\r\no7 = sin(o7)/(l7);\r\no8 = sin(o8)/(l8);\r\n\r\nosc = o1+o2+o3+o4+o5+o6+o7+o8;\r\nosc = clip(osc,-1.07,1.07);\r\nosc = asin(osc);\r\n\r\nosc*= 1+in2;\r\nout1 = osc*2;\r\nout2 =  slide(mgain,1000,1000)*selector(o_2,0.5*((1-l1)*c1+(1-l2)*c2+(1-l3)*c3+(1-l4)*c4+(1-l5)*c5+(1-l6)*c6+(1-l7)*c7+(1-l8)*c8),c1,c2,c3,c4,c5,c6,c7,c8);\r\n",
