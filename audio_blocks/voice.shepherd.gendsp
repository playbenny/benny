				"box" : 				{
					"code" : "require \"osc_functions\";\r\nBuffer shape(\"osc_shape_lookup\");\r\nBuffer mtof_lookup(\"mtof_lookup\");\r\nBuffer prm(\"voice_parameter_buffer\");\r\nBuffer detuning(\"detuning\");\r\n\r\nHistory cursor_s(0);\r\nHistory cursor_p(0);\r\nHistory ws,ce,wi,p_unstab,s_unstab,detune,mod1,mod2,cr,sr,porta,sha,sha_off(0),mgain;\r\nHistory gap(1),g12(12),iwi(1);\r\n\r\nHistory at,su,de,cu,lo,ve,ac,cu_t(0),cu_f(0);\r\nHistory ov(0);  //last non-zero velocity\r\nHistory oov(0); //just for detect change\r\nHistory otrig(0); //likewise\r\nHistory op(0); //old pitch\r\nHistory env(0);\r\nHistory attdone(1); \r\nHistory decdone(1); \r\nHistory target(0);\r\nHistory attrate(0.00001); \r\nHistory decrate(0.00001);\r\nHistory upda(0);\r\n\r\nParam voice_is(0, min=0,default=0,max=4096);\r\nParam voice_offset(0);\r\nParam p(0,min=0,default=64,max=128);\r\nParam v(0,min=0,default=0,max=1000);\r\nParam panic(0);\r\nParam latch(0);\r\nParam cont(1);\r\n\r\n\r\nc_p_r = 0.00001/(voice_is+20);\r\ncursor_p += c_p_r;\r\n\r\nif(upda<=0){\r\n\tupda = vectorsize+voice_is;\r\n\tws = peek(prm,voice_offset, 0,channels=1);\r\n\tce = 96*peek(prm,1+ voice_offset,0, channels=1);\r\n\twi = 4*peek(prm,2+ voice_offset,0, channels=1)+0.01;\r\n\tiwi = 1/(wi+1);\r\n\tgap = floor(1.99*peek(prm,3+ voice_offset,0, channels=1))+1;\r\n\tg12=gap*12;\r\n\tp_unstab = peek(prm,4+ voice_offset, 0,channels=1);\r\n\tp_unstab *= p_unstab*p_unstab*0.5;\r\n\tcr = p_unstab/(voice_is*5180+1000);\r\n\ts_unstab = peek(prm,5+ voice_offset,0, channels=1);\r\n\ts_unstab *= s_unstab;\r\n\tsr = s_unstab/(voice_offset*3260+4200);\r\n\tdetune = 48*peek(prm,6+ voice_offset,0, channels=1);\r\n\tmod1 = peek(prm,7+ voice_offset,0, channels=1);\r\n\tmod2 = peek(prm,8+voice_offset,0, channels=1);\r\n\tporta = 100*peek(prm,9+voice_offset,0, channels=1);\r\n\tporta = porta*porta;\r\n\tporta = mstosamps(porta);\r\n\tat = peek(prm, 10+voice_offset, 0, channels=1);\r\n\tat = (pow(1000,at)-1+0.0000000001)*60.06006006006006006006006006006;\r\n\tsu = peek(prm, 11+voice_offset, 0, channels=1);\r\n\tde = peek(prm, 12+voice_offset, 0, channels=1);\r\n\tde = (pow(1000,de)-1+0.0000000001)*60.06006006006006006006006006006;\r\n\tcu = peek(prm, 13+voice_offset, 0, channels=1);\r\n\tlo = peek(prm, 14+voice_offset, 0, channels=1);\r\n\tve = peek(prm, 15+voice_offset, 0, channels=1)*2 - 1;\r\n\tac = peek(prm, 16+voice_offset, 0, channels=1)*2 - 1;\t\r\n\tcu_f=cu*4;\t\r\n\tcu_t=floor(cu_f);\r\n\tcu_f-=cu_t;\r\n\tcu_t+=1;\r\n\tt=1+abs(cu-0.45)*23;\r\n\tattrate=t/mstosamps(at);\r\n\tdecrate=-t/mstosamps(de);\r\n\tmgain = 4*peek(prm,17+voice_offset,0, channels=1)-2;\r\n}\r\n\r\nupda -= cont;\r\n\r\ncursor_p += 0.000001*cr;\r\ncursor_s += sr;\r\ncursor_p=wrap(cursor_p);\r\ncursor_s=wrap(cursor_s);\r\n\r\npink_s = sample(detuning,cursor_s,0);\r\npink_p = sample(detuning,cursor_p,0);\r\n\r\n//sh = slide(ws,100,100);\r\n//sh += in2;\r\n//sh += (slide(s_unstab,100,100)*pink_s);\r\nsh = ws + s_unstab*pink_s;\r\nif(mod1>=0.5){\r\n\tif(mod1<0.66){\r\n\t\tsh+=in1;\r\n\t}else if(mod1<0.833){\r\n\t\tce+=64*in1;\r\n\t}else{\r\n\t\twi+=4*in1;\r\n\t}\r\n}\r\nif(mod2<0.33){\r\n\tsh+=in2;\r\n}else if(mod2<0.66){\r\n\tce+=in2;\r\n}else{\r\n\twi+=in2;\r\n}\r\nsdif = sh+sha_off - sha;\r\nif(sdif>0.5) sha_off-=1;\r\nif(sdif<-0.5) sha_off+=1;\r\nsha = slide(sh + sha_off,100,100);\r\nsin_l, tri_l, rec_l, pw = sample(shape, wrap(sha) , 0, channels=4);\r\n\r\nnote = p % g12;\r\nnotegap = (note - (ce % g12))/g12;\r\noct = floor(ce / g12) * g12 + note +0.0001;\r\n//oct - w* g12\r\n//pitch = peek(mtof_lookup,slide(oct,porta,porta), (voice_is % 8), channels=1);\r\n//pit = clip(pitch * pow(2, (slide(p_unstab,100,100)*pink_p*0.5)+(slide(detune*4,100,100))),0.0001,samplerate/3);\r\n\r\npitch = slide(oct,porta,porta) + (slide(p_unstab,100,100)*pink_p)+(slide(detune,100,100));\r\ntra=1;\r\nif(pitch<0){\r\n\ttra = floor(-pitch/12);\r\n\tpitch += 12*tra;\r\n\ttra = pow(2, -tra);\r\n}else{\r\n\tif(pitch>128) pitch=128;\r\n}\r\npit = 0.25*tra*peek(mtof_lookup,pitch, (voice_is % 8), channels=1, interp = \"linear\");\r\n\r\n\r\n\r\nopit = clip(abs(pit*(1+in1*(mod1<0.5))),0.0001,samplerate/3);\r\n\r\n//opit = pit;\r\ng2 = 2*gap;\r\nrec=0;\r\nrrec=0;\r\nsaw=0;\r\nsine=0;\r\nol = clip(1 - abs((notegap-2)/wi),0,1);\r\nif(ol>0){\r\n\tsine += ol * cycle(opit);\r\n\tif(mod1<0.166){\r\n\t\trec =  eptr_rect(pit,in1,pw, sync=0);\r\n\t}else if(mod1<0.33){\r\n\t\trec =  eptr_rect(pit,in1,pw, sync=1);\r\n\t}else if(mod1<0.5){ \r\n\t\trec =  eptr_rect(pit,in1,pw, sync=2);\r\n\t}else{\r\n\t\trec = eptr_rect(pit,0,pw);\r\n\t}\r\n\trrec += ol*rec;\r\n\tsaw +=  ol*integrator_saw(rec, opit, pw);\r\n}\r\nopit*=g2;\r\npit*=g2;\r\n//opit = 9999999;\r\nif(opit<11000){\r\n\tol = clip(1 - abs((notegap-1)/wi),0,1);\r\n\tif(ol>0){\r\n\t\tsine += ol * cycle(opit);\r\n\t\tif(mod1<0.166){\r\n\t\t\trec =  eptr_rect(pit,in1,pw, sync=0);\r\n\t\t}else if(mod1<0.33){\r\n\t\t\trec =  eptr_rect(pit,in1,pw, sync=1);\r\n\t\t}else if(mod1<0.5){\r\n\t\t\trec =  eptr_rect(pit,in1,pw, sync=2);\r\n\t\t}else{\r\n\t\t\trec = eptr_rect(pit,0,pw);\r\n\t\t}\r\n\t\trrec += ol*rec;\r\n\t\tsaw +=  ol*integrator_saw(rec, opit, pw);\r\n\t}\r\n\topit*=g2;\r\n\tpit*=g2;\r\n\r\n\tif(opit<11000){\r\n\t\tol = clip(1 - abs((notegap  )/wi),0,1);\r\n\t\tif(ol>0){\r\n\t\t\tsine += ol * cycle(opit);\r\n\t\t\tif(mod1<0.166){\r\n\t\t\t\trec =  eptr_rect(pit,in1,pw, sync=0);\r\n\t\t\t}else if(mod1<0.33){\r\n\t\t\t\trec =  eptr_rect(pit,in1,pw, sync=1);\r\n\t\t\t}else if(mod1<0.5){ \r\n\t\t\t\trec =  eptr_rect(pit,in1,pw, sync=2);\r\n\t\t\t}else{\r\n\t\t\t\trec = eptr_rect(pit,0,pw);\r\n\t\t\t}\r\n\t\t\trrec += ol*rec;\r\n\t\t\tsaw +=  ol*integrator_saw(rec, opit, pw);\r\n\t\t}\r\n\t\topit*=g2;\r\n\t\tpit*=g2;\r\n\r\n\t\tif(opit<11000){\r\n\t\t\tol = clip(1 - abs((notegap+1)/wi),0,1);\r\n\t\t\tif(ol>0){\r\n\t\t\t\tsine += ol * cycle(opit);\r\n\t\t\t\tif(mod1<0.166){\r\n\t\t\t\t\trec =  eptr_rect(pit,in1,pw, sync=0);\r\n\t\t\t\t}else if(mod1<0.33){\r\n\t\t\t\t\trec =  eptr_rect(pit,in1,pw, sync=1);\r\n\t\t\t\t}else if(mod1<0.5){ \r\n\t\t\t\t\trec =  eptr_rect(pit,in1,pw, sync=2);\r\n\t\t\t\t}else{\r\n\t\t\t\t\trec = eptr_rect(pit,0,pw);\r\n\t\t\t\t}\r\n\t\t\t\trrec += ol*rec;\r\n\t\t\t\tsaw +=  ol*integrator_saw(rec, opit, pw);\r\n\t\t\t}\r\n\t\t\topit*=g2;\r\n\t\t\tpit*=g2;\r\n\t\t\t\r\n\t\t\tif(opit<11000){\r\n\t\t\t\tol = clip(1 - abs((notegap+2)/wi),0,1);\r\n\t\t\t\tif(ol>0){\r\n\t\t\t\t\tif(mod1<0.166){\r\n\t\t\t\t\t\trec =  eptr_rect(pit,in1,pw, sync=0);\r\n\t\t\t\t\t}else if(mod1<0.33){\r\n\t\t\t\t\t\trec =  eptr_rect(pit,in1,pw, sync=1);\r\n\t\t\t\t\t}else if(mod1<0.5){ \r\n\t\t\t\t\t\trec =  eptr_rect(pit,in1,pw, sync=2);\r\n\t\t\t\t\t}else{\r\n\t\t\t\t\t\trec = eptr_rect(pit,0,pw);\r\n\t\t\t\t\t}\r\n\t\t\t\t\trrec += ol*rec;\r\n\t\t\t\t\tsaw +=  ol*integrator_saw(rec, opit, pw);\r\n\t\t\t\t\tsine += ol * cycle(opit);\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n}\r\n\r\nosc = sin_l * sine + tri_l * saw + rec_l * rrec;\r\nosc = osc* iwi;\r\n\r\ntrig=0;\r\nvi=0;\r\nif((delta(lo>0)>0)&&(env==0)){\r\n\tvi=1;\r\n\ttrig=1;\r\n\tupda=0;\r\n}\r\nlevscale = 0;\r\n\r\nif(v>0){\r\n\tlevscale = v * ve + (1-clip(ve,0,2));\r\n}\r\n\r\nif(su>0){\r\n\tvi += levscale; \r\n\tif(vi!=oov){ //follow behaviour\r\n\t\toov=vi;\r\n\t\tov=vi;\r\n\t\ttarget=vi;\r\n\t\tif(target<=env){\r\n\t\t\tattdone=1;\r\n\t\t\tdecdone=0;\r\n\t\t\tif((target == 0)&&(latch>2)) upda = 0;\r\n\t\t}else{\r\n\t\t\tdecdone=1;\r\n\t\t\tattdone=0;\r\n\t\t\tif((latch+(env==0))>1) upda = 0;\r\n\t\t}\r\n\t}\r\n}else{\r\n\ttrig += levscale;\r\n\tif((v!=oov)||(p!=op)){//(delta(trig>0)==1)){//trigger behaviour\r\n\t\tif(trig>0){\r\n\t\t\top=p;\r\n\t\t\tattdone=0;\r\n\t\t\tdecdone=1;\r\n\t\t\tif(latch != 0) upda = 0;\r\n\t\t\toov=v;\r\n\t\t\tov = levscale+ac*env;\r\n\t\t\tov = clip(ov,-4,4);\r\n\t\t\tov = ov - (ov*ov*ov*0.04166666);\r\n\t\t\ttarget=ov;\r\n\t\t}\r\n\t}\r\n}\r\n\r\n\r\nif(panic) {\r\n\ttarget=0;\r\n\tdecdone=0;\r\n\tdecrate=-0.001;\r\n\tenv*=0.999;\r\n}\r\n\r\nif(!attdone){\r\n\tt=clip(target-env,0,0.9999);\r\n\tif(t>0.0001){\r\n\t\tdx = selector(cu_t,(1-t)*(1-t*cu_f),1+t*(cu_f-1),1+cu_f*(t-1),(t+0.01)*(1+cu_f*(t-0.99)));//,(t+0.1)*(t+0.01));\r\n\t\tdx*=attrate;\r\n\t\tdx=clip(dx,0.0000001,t);\r\n\t\tenv+=dx;\r\n\t}else{\r\n\t\tenv=target;\r\n\t\tattdone=1;\r\n\t\tdecdone=0;\r\n\t\ttarget = su * v;\r\n\t}\r\n}\r\nif(!decdone){\r\n\tt=clip(target-env,-0.9999,0.9999);\r\n\tif(t<-0.001){\r\n\t\tdx = selector(cu_t,((t+1)*(t+1)-1)*(1-cu_f)+cu_f*t,t-cu_f*(t+1),-1-cu_f*t,-1-t*(1+cu_f+cu_f*t));\r\n\t\tdx*= -decrate;\r\n\t\tdx=clip(dx,t,-0.0000000001);\r\n\t\tenv+=dx;\r\n\t}else{\r\n\t\tenv=target;\r\n\t\tdecdone=1;\r\n\t\tif(lo){\r\n\t\t\tov = ov*lo;\r\n\t\t\tif(ov>0.001){\r\n\t\t\t\tattdone=0;\r\n\t\t\t\ttarget=ov*lo;\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n}\r\n//env = twopole_lp(env, cutoff=8000);\r\nout1 = slide(mgain,1000,1000)*osc*env; //twopole_lp(osc, cutoff=12000)*twopole_lp(env, cutoff=8000);\r\nout2 = env;",
