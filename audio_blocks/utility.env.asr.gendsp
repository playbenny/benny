				"box" : 				{
					"code" : "Buffer prm(\"voice_parameter_buffer\");\r\n\r\nHistory at,su,de,cu,lo,ve,ac,i1,i2,o2,cu_t(0),cu_a(0),cu_f(0),ga;\r\n\r\nHistory ov(0);  //last non-zero velocity\r\nHistory oov(0); //just for detect change\r\nHistory otrig(0); //likewise\r\nHistory otr(0); //for holding triggers until attdone\r\nHistory env(0);\r\nHistory attdone(1); \r\nHistory decdone(1); \r\nHistory target(0);\r\nHistory attrate(0.00001); \r\nHistory decrate(0.00001);\r\nHistory upda(0);\r\n\r\nParam voice_offset(0);\r\nParam tr(0);\r\nParam v(0);\r\nParam panic(0);\r\n\r\nif(upda<=0){\r\n\tupda = vectorsize + 4;\r\n\tat = peek(prm, voice_offset, 0, channels=1);\r\n\tat = (pow(1000,at)-1+0.0000000001)*60.06006006006006006006006006006;\r\n\tsu = peek(prm, 1+voice_offset, 0, channels=1);\r\n\tde = peek(prm, 2+voice_offset, 0, channels=1);\r\n\tde = (pow(1000,de)-1+0.0000000001)*60.06006006006006006006006006006;\r\n\tcu = peek(prm, 3+voice_offset, 0, channels=1);\r\n\tlo = peek(prm, 4+voice_offset, 0, channels=1);\r\n\tve = peek(prm, 5+voice_offset, 0, channels=1)*2 - 1;\r\n\tac = peek(prm, 6+voice_offset, 0, channels=1)*2 - 1;\r\n\ti1 = floor(5*peek(prm, 7+voice_offset, 0, channels=1));\r\n\ti2 = floor(3*peek(prm, 8+voice_offset, 0, channels=1));\r\n\to2 = floor(4*peek(prm, 9+voice_offset, 0, channels=1));\r\n\tga = 2*peek(prm, 10+voice_offset, 0, channels=1)-1;\r\n\tsame = peek(prm, 11+voice_offset, 0, channels=1)>0.5;\r\n\tcu_f=cu*4;\t\r\n\tcu_t=floor(cu_f);\r\n\tcu_f-=cu_t;\r\n\tcu_t+=1;\r\n\tcu_a = same ? cu_t : (5-cu_t);\r\n\tt=1+abs(cu-0.45)*23;\r\n\tattrate=t/mstosamps(at);\r\n\tdecrate=-t/mstosamps(de);\t\r\n}\r\nupda -= 1;\r\n\r\nvi=0;\r\nlevscale = 0;\r\n/*if(attdone){\r\n\tif(otr!=tr){\r\n\t\ttrig=tr;\r\n\t\totrig = 0;\r\n\t\totr=tr;\r\n\t}\r\n}*/\r\ntrig=0;\r\nif(delta(tr)) trig += tr*attdone;\r\nif((delta(lo>0)>0)&&(env==0)){\r\n\ttrig=1;\r\n}\r\natr = attrate; der = decrate;\r\n\r\nif(i1==0){\r\n\ttrig+=(in1>0.00000001);\r\n\tlevscale += trig;\r\n}else if(i1==1){\r\n\tlevscale += trig;\r\n\ttrig = 0;\r\n\totrig=0;\r\n\tvi += in1;\r\n\tcu_t = clip(cu_t,2,3);\r\n\tcu_f = clip(cu_f,0.001,0.999);\r\n}else if(i1==2){\r\n\tatr /= (1+in1);\r\n}else if(i1==3){\r\n\tder /= (1+in1);\r\n}else if(i1==4){\r\n\tatr /= (1+in1);\r\n\tder /= (1+in1);\r\n}\r\nif(in2!=0){\r\n\tif(i2==0){\r\n\t\tatr /= (1+in2);\r\n\t}else if(i2==1){\r\n\t\tder /= (1+in2);\r\n\t}else if(i2==2){\r\n\t\tatr /= (1+in2);\r\n\t\tder /= (1+in2);\r\n\t}\r\n}\r\nif(panic) {\r\n\ttarget=0;\r\n\tdecdone=0;\r\n\tder=-0.001;\r\n\tenv*=0.999;\r\n}\r\n\r\nif(v>0){\r\n\tlevscale = v * ve + (1-clip(ve,0,2));\r\n\tif(su>0){\r\n\t\tvi += levscale; \r\n\t}else{\r\n\t\ttrig += levscale;//v;//\r\n\t}\r\n}else{\r\n\tif((attdone) && (su>0) && (i1!=1)){\r\n\t\ttarget = 0;\r\n\t\tdecdone = 0;\r\n\t}\r\n}\r\nif(vi!=oov){ //follow behaviour\r\n\toov=vi;\r\n\tov=vi;\r\n\ttarget=vi;\r\n\tif(target<env){\r\n\t\tattdone=1;\r\n\t\tdecdone=0;\r\n\t}else if(target>env){\r\n\t\tdecdone=1;\r\n\t\tattdone=0;\r\n\t}\r\n}\r\n\r\nif((trig!=otrig)&&(attdone==1)){//trigger behaviour\r\n\tif(trig!=0){\r\n\t\tattdone=0;\r\n\t\tdecdone=1;\r\n\t\totrig=trig;\r\n\t\t//levscale = (tr+v+trig) * ve + (1-clip(ve,0,2));\r\n\t\tov = levscale+ac*env;\r\n\t\tov /= 2;\r\n\t\tov = clip(ov,-2,2);\r\n\t\tov = ov - (ov*ov*ov/3);\r\n\t\tov *= 2;\r\n\t\ttarget=ov;\r\n\t}else{\r\n\t\ttrig = 0;\r\n\t\totrig = 0;\r\n\t}\r\n}\r\nif(!attdone){\r\n\tt=clip(target-env,-0.9999,0.9999);\r\n\tif(t>0.00001){\r\n\t\tdx = selector(cu_a,(1-t)*(1-t*cu_f),1+t*(cu_f-1),1+cu_f*(t-1),(t+0.01)*(1+cu_f*(t-0.99)));//,(t+0.1)*(t+0.01));\r\n\t\tdx*=atr;\r\n\t\tdx=clip(dx,0.0000001,t);\r\n\t\tenv+=dx;\r\n\t}else{\r\n\t\tenv=target;\r\n\t\tattdone=1;\r\n\t\tif(i1!=1){\r\n\t\t\tdecdone=0;\r\n\t\t\ttarget = su*v;\r\n\t\t\ttrig=0;\r\n\t\t}\r\n\t}\r\n}\r\nif(!decdone){\r\n\tt=clip(target-env,-0.9999,0.9999);\r\n\tif(t<-0.001){\r\n\t\tdx = selector(cu_t,((t+1)*(t+1)-1)*(1-cu_f)+cu_f*t,t-cu_f*(t+1),-1-cu_f*t,-1-t*(1+cu_f+cu_f*t));\r\n\t\tdx*= -der;\r\n\t\tdx=clip(dx,t,-0.0000000001);\r\n\t\tenv+=dx;\r\n\t}else{\r\n\t\tenv=target;\r\n\t\tdecdone=1;\r\n\t\tif(lo){\r\n\t\t\tov = ov*lo;\r\n\t\t\tif(ov>0.001){\r\n\t\t\t\tattdone=0;\r\n\t\t\t\ttarget=ov*lo;\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n}\r\n\r\nout1 = ga*env;\r\n\r\nif(o2==0){\r\n\tout2 = target;//levscale;//vi+trig;\r\n}else if(o2==1){\r\n\tout2 = attdone;\r\n}else if(o2==2){\r\n\tout2 = decdone;\r\n}else{\r\n\tout2 = delta(env);\r\n}\r\n\r\n\r\nout3 = attdone;\r\nout4 = decdone;\r\nout5 = (env<0.0001) * (attdone) * (lo==0)* (in1==0)*(in2==0);",
