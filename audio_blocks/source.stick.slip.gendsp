					"code" : "Buffer mtof_lookup(\"mtof_lookup\");\r\nBuffer prm(\"voice_parameter_buffer\");\r\nBuffer detuning(\"detuning\");\r\n\r\nHistory pos, smoo, mass, oomass, tens, stat, mov, text, mod;\r\nHistory x, dx;\r\nHistory op, upda(0);\r\n\r\nParam voice_is(0, min=0,default=0,max=4096);\r\nParam voice_offset(0);\r\nParam p(0,min=0,default=60,max=128);\r\n//Param v(0,min=0,default=0,max=1000);\r\n\r\nif((p!=op)){\r\n\ttp = p;\r\n\top = p;\r\n\tov = v;\r\n}\r\nupda+=1;\r\nif(upda>vectorsize){\r\n\tupda = -voice_is;\r\n\tpos = -1 + 2*peek(prm,voice_offset, 0,channels=1);\r\n\tsmoo = peek(prm,1+ voice_offset,0, channels=1);\r\n\tmass = peek(prm,2+ voice_offset, 0,channels=1);\r\n\ttens = peek(prm,3+ voice_offset,0, channels=1);\r\n\tstat = peek(prm,4+ voice_offset,0, channels=1);\r\n\tmov  = peek(prm,5+ voice_offset,0, channels=1);\r\n\ttext = peek(prm,6+ voice_offset,0, channels=1);\r\n\tmod  = floor(4*peek(prm,7+ voice_offset,0, channels=1));\r\n\tmass = (pow(100, mass) - 1)*0.01010101010101010101010101010101*99.99+0.01;\r\n\ttens = (pow(100, tens) - 1)*0.01010101010101010101010101010101;\r\n\tstat = (pow(100, stat) - 1)*0.001010101010101010101010101010101;\r\n\tmov  = (pow(100, mov ) - 1)*0.001010101010101010101010101010101;\r\n\ttext = (pow(100, text) - 1)*1.010101010101010101010101010101;\r\n\tsmoo = (pow(1000, smoo) - 1)*0.001001001001001001001001001001*mstosamps(1000);\r\n\toomass = 1/ mass;\r\n\tmov *= oomass;\r\n}\r\nstop=0;\r\nd = slide((pos+in1),smoo,smoo) - x;\r\nf = d * tens;\r\na = 0;\r\nt = 1 + text * peek(detuning,(1+x)*8192,0);\r\nif(dx==0){\r\n\tif(abs(f)>(stat*mass*t)){\r\n\t\ta = f * oomass;\r\n\t}\r\n}else{\r\n\ta = f * oomass;\r\n}\r\nodx = dx;\r\ndx += a;\r\nif(dx>0){\r\n\tdx-=mov*t;\r\n\tif(dx<0){\r\n\t\tdx=0;\r\n\t\tstop=1;\r\n\t}\r\n}else{\r\n\tdx+=mov*t;\r\n\tif(dx>0){\r\n\t\tdx=0;\r\n\t\tstop=1;\r\n\t}\r\n}\r\ndx_xing = (odx!=0) && ((odx>0)!=(dx>0));\r\nx += dx;\r\n\r\nif(dx_xing){\r\n\tdx = 0; //force it to apply static friction\r\n\t//tr